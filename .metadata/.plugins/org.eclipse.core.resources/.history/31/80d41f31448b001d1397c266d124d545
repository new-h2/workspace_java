package book;

import java.util.Date;

public class BookDTO {
	String bookNo, bookTitle, bookAuthor, bookPublisher;
	Date bookTime;
	int bookPrice;
	
	public BookDTO(String bookNo, String bookTitle, String bookAuthor, String bookPublisher, Date bookTime,
			int bookPrice) {
		super();
		this.bookNo = bookNo;
		this.bookTitle = bookTitle;
		this.bookAuthor = bookAuthor;
		this.bookPublisher = bookPublisher;
		this.bookTime = bookTime;
		this.bookPrice = bookPrice;
	}

	@Override
	public String toString() {
		return "BookDTO [bookNo=" + bookNo + ", bookTitle=" + bookTitle + ", bookAuthor=" + bookAuthor
				+ ", bookPublisher=" + bookPublisher + ", bookTime=" + bookTime + ", bookPrice=" + bookPrice
				+ ", toString()=" + super.toString() + "]";
	} 
	
	
}
