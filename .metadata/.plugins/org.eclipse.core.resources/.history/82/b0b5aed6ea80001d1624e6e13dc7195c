package chap15;

import java.io.IOException;

public class SystemInTest {

	public static void main(String[] args) throws IOException {
		//모든 IOException 처리
		//콘솔창에 입력+ 엔터가능

		System.out.println("사용자 입력 대기증");
		while(true) {
			int data2  = System.in.read();
			System.out.println((char)data2);
		}
		
	}

}
